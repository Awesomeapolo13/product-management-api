{
  "name": "product-management-api",
  "version": "1.0.0",
  "description": "The API that has been developed for wokring with products, their quantity, remain and etc",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/public/index.js",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register index.ts",
    "lint": "eslint src/**",
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.ts --coverage",
    "lint:fix": "eslint src/** --fix",
    "build": "tsc",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Awesomeapolo13/product-management-api.git"
  },
  "keywords": [
    "api",
    "products",
    "remains"
  ],
  "author": "Aleksey Logachev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Awesomeapolo13/product-management-api/issues"
  },
  "homepage": "https://github.com/Awesomeapolo13/product-management-api#readme",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "tslog": "^4.8.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
